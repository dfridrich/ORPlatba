{
    "name": "honzito/qr-platba",
    "type": "library",
    "description": "Generování QR Plateb a QR Faktur v PHP.",
    "keywords": [
        "php",
        "qr",
        "platba"
    ],
    "homepage": "https://github.com/dfridrich/QRPlatba",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Fridrich",
            "email": "fridrich.dennis@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/honzito/iban-validation.git"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "honzito/iban-validation": "@dev",
        "honzito/bacon-qr-code-basis": ">=2023.208.1",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Defr\\QRPlatba\\": "src/"
        },
        "files": [
            "src/bcmod.php"
        ]
